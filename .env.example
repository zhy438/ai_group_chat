# AI Group Chat configuration example

# App
APP_NAME=AI群聊
DEBUG=true

# Database
DATABASE_URL=postgresql://admin:password@localhost:5432/ai_chat_db

# AI provider (OpenAI-compatible endpoint)
AI_API_BASE=https://aihubmix.com/v1
AI_API_KEY=your_api_key_here

# Default model
DEFAULT_MODEL=mimo-v2-flash-free

# Discussion
MAX_DISCUSSION_ROUNDS=5

# LangSmith (optional)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com
LANGCHAIN_API_KEY=
LANGCHAIN_PROJECT=ai-group-chat
